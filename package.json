{
  "name": "remark-math",
  "description": "Math Inline and Block supporting for Remark",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "test": "jest --coverage",
    "test:api": "jest",
    "test:watch": "jest --watch",
    "pretest": "npm run lint --silent",
    "lint": "standard",
    "demo:webpack": "webpack-dev-server --config examples/webpack/webpack.config.js",
    "demo:nodejs": "node examples/nodejs/process.js",
    "release": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rokt33r/remark-math.git"
  },
  "keywords": [
    "markdown",
    "remark",
    "math",
    "katex",
    "latex",
    "tex"
  ],
  "author": "Junyoung Choi <fluke8259@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rokt33r/remark-math/issues"
  },
  "homepage": "https://github.com/Rokt33r/remark-math#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "css-loader": "^0.28.4",
    "eslint-plugin-jest": "^20.0.3",
    "file-loader": "^0.11.2",
    "github-markdown-css": "^2.6.0",
    "hastscript": "^3.0.1",
    "jest": "^23.6.0",
    "katex": "^0.9.0",
    "lerna": "^2.0.0-rc.5",
    "rehype-parse": "^4.0.0",
    "rehype-stringify": "^3.0.0",
    "remark-html": "^6.0.1",
    "remark-parse": "^4.0.0",
    "remark-rehype": "^2.0.1",
    "remark-stringify": "^3.0.1",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "unified": "^6.1.5",
    "unist-builder": "^1.0.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {},
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "jest"
    ],
    "envs": [
      "jest"
    ]
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "packages/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
